Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"solveME", "[", 
    RowBox[{
    "nmax_", ",", "nuIC_", ",", "krBirth_", ",", "krDeath_", ",", "nTimes_", 
     ",", "dt_"}], "]"}], ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", "pSt", "}"}], "\[IndentingNewLine]", ",", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"Solution", " ", "structure"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"pSt", "=", 
       RowBox[{"Association", "[", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Do", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"pSt", "[", "iTime", "]"}], "=", 
          RowBox[{"Association", "[", "]"}]}], ";"}], "\[IndentingNewLine]", 
        ",", 
        RowBox[{"{", 
         RowBox[{"iTime", ",", "1", ",", "nTimes"}], "}"}]}], "]"}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", "IC", " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"Do", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"pSt", "[", "1", "]"}], "[", "n", "]"}], "=", 
          RowBox[{"fpt", "[", 
           RowBox[{"n", ",", "nuIC"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
        ",", 
        RowBox[{"{", 
         RowBox[{"n", ",", "0", ",", "nmax"}], "}"}]}], "]"}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", "Go", " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"Do", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"pSt", "[", "iTime", "]"}], "[", "0", "]"}], "=", 
          RowBox[{
           RowBox[{
            RowBox[{"pSt", "[", 
             RowBox[{"iTime", "-", "1"}], "]"}], "[", "0", "]"}], "+", 
           RowBox[{"dt", "*", "krDeath", "*", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"pSt", "[", 
               RowBox[{"iTime", "-", "1"}], "]"}], "[", "1", "]"}], 
             ")"}]}]}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"Do", "[", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"pSt", "[", "iTime", "]"}], "[", "n", "]"}], "=", 
             RowBox[{
              RowBox[{
               RowBox[{"pSt", "[", 
                RowBox[{"iTime", "-", "1"}], "]"}], "[", "n", "]"}], "+", 
              RowBox[{"dt", "*", 
               RowBox[{"(", 
                RowBox[{
                 RowBox[{"krDeath", "*", 
                  RowBox[{
                   RowBox[{"pSt", "[", 
                    RowBox[{"iTime", "-", "1"}], "]"}], "[", 
                   RowBox[{"n", "+", "1"}], "]"}]}], "-", 
                 RowBox[{"krDeath", "*", 
                  RowBox[{
                   RowBox[{"pSt", "[", 
                    RowBox[{"iTime", "-", "1"}], "]"}], "[", "n", "]"}]}], 
                 "+", 
                 RowBox[{"krBirth", "*", 
                  RowBox[{
                   RowBox[{"pSt", "[", 
                    RowBox[{"iTime", "-", "1"}], "]"}], "[", 
                   RowBox[{"n", "-", "1"}], "]"}]}], "-", 
                 RowBox[{"krBirth", "*", 
                  RowBox[{
                   RowBox[{"pSt", "[", 
                    RowBox[{"iTime", "-", "1"}], "]"}], "[", "n", "]"}]}]}], 
                ")"}]}]}]}], ";"}], "\[IndentingNewLine]", ",", 
           RowBox[{"{", 
            RowBox[{"n", ",", "1", ",", 
             RowBox[{"nmax", "-", "1"}]}], "}"}]}], "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           RowBox[{"pSt", "[", "iTime", "]"}], "[", "nmax", "]"}], "=", 
          RowBox[{
           RowBox[{
            RowBox[{"pSt", "[", 
             RowBox[{"iTime", "-", "1"}], "]"}], "[", "nmax", "]"}], "+", 
           RowBox[{"dt", "*", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{
               RowBox[{"-", "krDeath"}], "*", 
               RowBox[{
                RowBox[{"pSt", "[", 
                 RowBox[{"iTime", "-", "1"}], "]"}], "[", "nmax", "]"}]}], 
              "+", 
              RowBox[{"krBirth", "*", 
               RowBox[{
                RowBox[{"pSt", "[", 
                 RowBox[{"iTime", "-", "1"}], "]"}], "[", 
                RowBox[{"nmax", "-", "1"}], "]"}]}]}], ")"}]}]}]}], ";"}], 
        "\[IndentingNewLine]", ",", 
        RowBox[{"{", 
         RowBox[{"iTime", ",", "2", ",", "nTimes"}], "}"}]}], "]"}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "pSt", "]"}], ";"}]}], "\[IndentingNewLine]", 
    "]"}]}], ";"}]], "Input",
 CellChangeTimes->{
